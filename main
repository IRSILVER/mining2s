-- Scripts:

local function CBEI_fake_script() -- bt1.LocalScript 
	local script = Instance.new('LocalScript', bt1)

	local i = script.Parent
	function notify(msg)
		game.StarterGui:SetCore("SendNotification", {
			Title = "Mining simulator 2";         
			Text = msg;         
			Duration = 3;     
		})
	end
	_G.On = false
	i.MouseButton1Click:Connect(function()
		if _G.On == false then
			_G.On = true
			loadstring(game:HttpGet("https://raw.githubusercontent.com/TwomadJR/mining2s/main/farm"))()
		elseif _G.On == true then
			_G.On = false
			loadstring(game:HttpGet("https://raw.githubusercontent.com/TwomadJR/mining2s/main/farm"))()
			notify("Autofarm disabled")
		end
