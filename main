local function NHLQBW_fake_script() -- Main.not 
	local script = Instance.new('LocalScript', Main)

	wait(0.2)
	function notify(msg)
		game.StarterGui:SetCore("SendNotification", {
			Title = "Mining simulator 2";         
			Text = msg;         
			Duration = 3;     
		})
	end
	notify("Script loaded, Enjoy")
end
coroutine.wrap(NHLQBW_fake_script)()
