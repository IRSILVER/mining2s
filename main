local function IZRVQOF_fake_script() -- Button1.LocalScript 
	local script = Instance.new('LocalScript', Button1)

	local oth = script.Parent.Parent.Parent.th
	local tw = script.Parent.Parent.Parent.tw
	local fo = script.Parent.Parent.Parent.fo
	script.Parent.MouseButton1Click:Connect(function()
		if tw.Visible == true then
			tw.Visible = false
		elseif tw.Visible == false then
			tw.Visible = true
			oth.Visible = false
			fo.Visible = false
		end
	end)
end
