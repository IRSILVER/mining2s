-- Scripts:

local function HCDT_fake_script() -- close.LocalScript 
	local script = Instance.new('LocalScript', close)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Parent.Parent:Destroy()
	end)
end
coroutine.wrap(HCDT_fake_script)()
local function CNNYDF_fake_script() -- bt1.LocalScript 
	local script = Instance.new('LocalScript', bt1)

	local i = script.Parent
	function notify(msg)
		game.StarterGui:SetCore("SendNotification", {
			Title = "Mining simulator 2";         
			Text = msg;         
			Duration = 3;     
		})
	end
	i.MouseButton1Click:Connect(function()
		if _G.On == true then
			_G.On = false
			loadstring(game:HttpGet("https://raw.githubusercontent.com/TwomadJR/mining2s/main/farm"))()
			notify("Autofarm Disabled")
		elseif _G.On == false then
			_G.On = true
			loadstring(game:HttpGet("https://raw.githubusercontent.com/TwomadJR/mining2s/main/farm"))()
		end
	end)
end
coroutine.wrap(CNNYDF_fake_script)()
local function UKSTV_fake_script() -- Button1.LocalScript 
	local script = Instance.new('LocalScript', Button1)

	local oth = script.Parent.Parent.Parent.th
	local tw = script.Parent.Parent.Parent.tw
	script.Parent.MouseButton1Click:Connect(function()
		if tw.Visible == true then
			tw.Visible = false
		elseif tw.Visible == false then
			tw.Visible = true
			oth.Visible = false
		end
	end)
end
coroutine.wrap(UKSTV_fake_script)()
